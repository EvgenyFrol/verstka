footer.footer
  .footer__wrapper
    .footer__logoImg
      img.footer__logo(src="/images/footer-logo.png" alt="footer__logo")

    .footer__menu
      .footer__socialLink
        select.footer__select
          option.footer__option(value="1") Русский
          option.footer__option(value="2") English
        p.footer__select--text #
          span.footer__span UberEats
        .footer__selectLangSocial
          each item in logoLink
            a(href= item.href target="_blank")
              img.footer__selectLangSocialLink(src= item.logo alt= item.alt)

      .footer__aboutUberEats
        each item in dataAbout
          a.footer__aboutUberEatsLinks.footer__links(href="#")= item.link

      .footer__help
        each item in information
          a.footer__helpLinks.footer__links(href="#")= item.inf

    .footer__app
      each item in appStore
        button.footer__button.button__app
          a(href= item.link target="_blank")
            img.app__logo(src= item.logo alt="app__logo")

    .footer__footer
      each item in foot
        p.footer__footerText= item.text
